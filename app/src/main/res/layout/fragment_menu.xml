<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layoutDirection="ltr"
    android:background="@drawable/background"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".fragment.MenuFragment">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_32sdp"
        android:layout_marginEnd="@dimen/_13sdp"
        android:layout_marginStart="@dimen/_155sdp"
        android:text="الملف الشخصي"
        android:fontFamily="@font/tajawal_bold"
        android:textColor="#000000"
        android:textSize="@dimen/_16sdp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.717"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="@dimen/_252sdp"
        android:layout_height="@dimen/_422sdp"
        android:layout_marginTop="@dimen/_6sdp"
        android:layout_marginBottom="@dimen/_102sdp"
        android:background="@drawable/field_background_shape"
        android:shadowColor="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView17"
        app:layout_constraintVertical_bias="0.128" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="@dimen/_11sdp"
        android:layout_height="@dimen/_12sdp"
        android:layout_marginTop="@dimen/_3sdp"
        android:layout_marginEnd="@dimen/_16sdp"
        android:layout_marginStart="@dimen/_7sdp"
        app:layout_constraintEnd_toEndOf="@+id/textView18"
        app:layout_constraintStart_toEndOf="@+id/textView19"
        app:layout_constraintTop_toTopOf="@+id/textView19"
        app:srcCompat="@drawable/user_name_orange" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_138sdp"
        android:layout_marginTop="@dimen/_12sdp"
        android:text="اسم المستخدم"
        android:fontFamily="@font/tajawal_bold"
        android:textSize="@dimen/_12sdp"
        android:textColor="#FF6F00"
        app:layout_constraintStart_toStartOf="@+id/textView18"
        app:layout_constraintTop_toTopOf="@+id/textView18" />

    <EditText
        android:id="@+id/userNameEd"
        android:layout_width="@dimen/_198sdp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_10sdp"
        android:layout_marginEnd="@dimen/_41sdp"
        android:layout_marginStart="@dimen/_12sdp"
        android:ems="10"
        android:inputType="textPersonName"
        android:hint="الاسم الفعلي"
        android:fontFamily="@font/tajawal_regular"
        android:textSize="@dimen/_9sdp"
        app:layout_constraintEnd_toEndOf="@+id/textView18"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="@+id/textView18"
        app:layout_constraintTop_toBottomOf="@+id/textView19" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="@dimen/_11sdp"
        android:layout_height="@dimen/_12sdp"
        android:layout_marginTop="@dimen/_3sdp"
        android:layout_marginEnd="@dimen/_16sdp"
        android:layout_marginStart="@dimen/_7sdp"
        app:layout_constraintEnd_toEndOf="@+id/textView18"
        app:layout_constraintHorizontal_bias="0.344"
        app:layout_constraintStart_toEndOf="@+id/textView20"
        app:layout_constraintTop_toTopOf="@+id/textView20"
        app:srcCompat="@drawable/phone_orange" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_163sdp"
        android:layout_marginTop="@dimen/_30sdp"
        android:fontFamily="@font/tajawal_bold"
        android:text="رقم الجوال"
        android:textColor="#FF6F00"
        android:textSize="@dimen/_12sdp"
        app:layout_constraintStart_toStartOf="@+id/textView18"
        app:layout_constraintTop_toBottomOf="@+id/userNameEd" />

    <EditText
        android:id="@+id/phoneLoginEt"
        android:layout_width="@dimen/_198sdp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_10sdp"
        android:layout_marginEnd="@dimen/_41sdp"
        android:layout_marginStart="@dimen/_12sdp"
        android:ems="10"
        android:hint="الرقم الفعلي"
        android:gravity="right"
        android:fontFamily="@font/tajawal_regular"
        android:textSize="@dimen/_9sdp"
        android:inputType="phone"
        app:layout_constraintEnd_toEndOf="@+id/textView18"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="@+id/textView18"
        app:layout_constraintTop_toBottomOf="@+id/textView20" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="@dimen/_11sdp"
        android:layout_height="@dimen/_12sdp"
        android:layout_marginTop="@dimen/_3sdp"
        android:layout_marginEnd="@dimen/_16sdp"
        android:layout_marginStart="@dimen/_7sdp"
        app:layout_constraintEnd_toEndOf="@+id/textView18"
        app:layout_constraintHorizontal_bias="0.333"
        app:layout_constraintStart_toEndOf="@+id/textView21"
        app:layout_constraintTop_toTopOf="@+id/textView21"
        app:srcCompat="@drawable/gmail_orange" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_138sdp"
        android:layout_marginTop="@dimen/_30sdp"
        android:fontFamily="@font/tajawal_bold"
        android:text="البريد الالكتروني"
        android:textColor="#FF6F00"
        android:textSize="@dimen/_12sdp"
        app:layout_constraintStart_toStartOf="@+id/textView18"
        app:layout_constraintTop_toBottomOf="@+id/phoneLoginEt" />

    <EditText
        android:id="@+id/emailEd"
        android:layout_width="@dimen/_198sdp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_10sdp"
        android:layout_marginEnd="@dimen/_41sdp"
        android:layout_marginStart="@dimen/_12sdp"
        android:ems="10"
        android:hint="البريد الفعلي"
        android:gravity="right"
        android:fontFamily="@font/tajawal_regular"
        android:textSize="@dimen/_9sdp"
        android:inputType="textEmailAddress"
        app:layout_constraintEnd_toEndOf="@+id/textView18"
        app:layout_constraintStart_toStartOf="@+id/textView18"
        app:layout_constraintTop_toBottomOf="@+id/textView21" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="@dimen/_11sdp"
        android:layout_height="@dimen/_12sdp"
        android:layout_marginTop="@dimen/_3sdp"
        android:layout_marginEnd="@dimen/_16sdp"
        android:layout_marginStart="@dimen/_7sdp"
        app:layout_constraintEnd_toEndOf="@+id/textView18"
        app:layout_constraintHorizontal_bias="0.344"
        app:layout_constraintStart_toEndOf="@+id/textView22"
        app:layout_constraintTop_toTopOf="@+id/textView22"
        app:srcCompat="@drawable/password_orange" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_159sdp"
        android:layout_marginTop="@dimen/_30sdp"
        android:fontFamily="@font/tajawal_bold"
        android:text="كلمة المرور"
        android:textColor="#FF6F00"
        android:textSize="@dimen/_12sdp"
        app:layout_constraintStart_toStartOf="@+id/textView18"
        app:layout_constraintTop_toBottomOf="@+id/emailEd" />

    <EditText
        android:id="@+id/editTextTextPassword"
        android:layout_width="@dimen/_198sdp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_10sdp"
        android:layout_marginEnd="@dimen/_41sdp"
        android:layout_marginStart="@dimen/_12sdp"
        android:ems="10"
        android:hint="كلمة المرور القديمة"
        android:gravity="right"
        android:fontFamily="@font/tajawal_regular"
        android:textSize="@dimen/_9sdp"
        android:inputType="textPassword"
        app:layout_constraintEnd_toEndOf="@+id/textView18"
        app:layout_constraintHorizontal_bias="0.487"
        app:layout_constraintStart_toStartOf="@+id/textView18"
        app:layout_constraintTop_toBottomOf="@+id/textView22" />

    <EditText
        android:id="@+id/editTextTextPassword2"
        android:layout_width="@dimen/_198sdp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="كلمة المرور الجديدة"
        android:gravity="right"
        android:fontFamily="@font/tajawal_regular"
        android:textSize="@dimen/_9sdp"
        android:inputType="textPassword"
        app:layout_constraintStart_toStartOf="@+id/editTextTextPassword"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPassword" />

    <EditText
        android:id="@+id/editTextTextPassword3"
        android:layout_width="@dimen/_198sdp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="تأكيد كلمة المرور الجديدة"
        android:gravity="right"
        android:fontFamily="@font/tajawal_regular"
        android:textSize="@dimen/_9sdp"
        android:inputType="textPassword"
        app:layout_constraintStart_toStartOf="@+id/editTextTextPassword2"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPassword2" />

    <Button
        android:id="@+id/confirmChangeBtn"
        android:layout_width="@dimen/_198sdp"
        android:layout_height="@dimen/_37sdp"
        android:layout_marginTop="@dimen/_12sdp"
        android:layout_marginStart="@dimen/_26sdp"
        android:layout_marginEnd="@dimen/_26sdp"
        android:background="@drawable/padding_btn"
        android:fontFamily="@font/tajawal_bold"
        android:text="تأكيد التغييرات"
        android:textSize="@dimen/_16sdp"
        app:layout_constraintEnd_toEndOf="@+id/textView18"
        app:layout_constraintHorizontal_bias="0.422"
        app:layout_constraintStart_toStartOf="@+id/textView18"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPassword3" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>